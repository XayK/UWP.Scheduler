<UserControl
    x:Class="SchedulingApp.Controls.MissionThumbControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:SchedulingApp.Controls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewmodel="using:SchedulingApp.Presenter.Entities"
    d:DataContext="{d:DesignInstance viewmodel:MissionViewModel}"
    d:DesignHeight="300"
    d:DesignWidth="400"
    mc:Ignorable="d">

    <Grid>
        <Border Background="{ThemeResource SystemAccentColorLight3}" BorderBrush="#cdcdcd" />
    </Grid>
</UserControl>
